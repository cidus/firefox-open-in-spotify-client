{
  "name": "firefox-open-in-spotify-client",
  "version": "1.3.0",
  "devDependencies": {
    "web-ext": "^2.9.1"
  },
  "scripts": {
    "build": "web-ext build -s src/ --overwrite-dest",
    "run": "web-ext run -s src/",
    "runwin": "node node_modules\\.bin\\web-ext run -s src/",
    "publish": "source AMOKEYS && web-ext sign -s src/ --api-key $AMOKEY --api-secret $AMOSECRET"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joaomsa/firefox-open-in-spotify-client.git"
  },
  "keywords": [
    "webextension",
    "webext",
    "spotify",
    "firefox"
  ],
  "bugs": {
    "url": "https://github.com/joaomsa/firefox-open-in-spotify-client/issues"
  },
  "homepage": "https://github.com/joaomsa/firefox-open-in-spotify-client#readme"
}
